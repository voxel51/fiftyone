{
    "name": "e2e",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "dependencies": {},
    "scripts": {
        "start": "ts-node -r source-map-support/register index.ts",
        "start-test-server": "FIFTYONE_DATABASE_NAME=cypress python ../fiftyone/server/main.py --address 0.0.0.0 --port 8787",
        "start:debug": "concurrently npm:start:debug:server npm:start:debug:app npm:start:debug:cypress",
        "start:debug:app": "FIFTYONE_DEFAULT_APP_PORT=8787 VITE_API=http://localhost:8989 yarn --cwd ../app dev --host 0.0.0.0",
        "start:debug:cypress": "FIFTYONE_DEFAULT_APP_PORT=8989 cypress open --e2e --browser chrome",
        "start:debug:server": "FIFTYONE_DATABASE_NAME=cypress python ../fiftyone/server/main.py --address 0.0.0.0 --port 8989",
        "start:watch": "nodemon -r source-map-support/register index.ts --watch lib --watch cypress --watch cypress.config.ts --watch index.ts -e js,jsx,ts,tsx,py,json"
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/cypress": "^1.1.3",
        "@types/express": "^4.17.17",
        "@types/isomorphic-fetch": "^0.0.36",
        "@types/node": "^18.15.3",
        "@types/wait-on": "^5.3.1",
        "body-parser": "^1.20.2",
        "concurrently": "^8.2.0",
        "cors": "^2.8.5",
        "cypress": "^12.14.0",
        "cypress-promise": "^1.1.0",
        "cypress-visual-regression": "^3.0.0",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "isomorphic-fetch": "^3.0.0",
        "nodemon": "^2.0.21",
        "prettier": "^2.8.4",
        "source-map-support": "^0.5.21",
        "ts-dedent": "^2.2.0",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.1.2",
        "typescript": "^4.9.5",
        "wait-on": "^7.0.1"
    }
}
